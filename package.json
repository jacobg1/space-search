{
  "name": "space-search",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"**/*.js\" && prettier --write \"**/*.vue\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.6.10",
    "vue-pure-lightbox": "^3.1.0",
    "vue-router": "^3.0.6",
    "vue2-waterfall": "^2.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "sass-loader": "^7.0.1",
    "vue-particles": "^1.0.9",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobg1/SpaceSearch"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "ignore",
          "named": "ignore",
          "asyncArrow": "ignore"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
